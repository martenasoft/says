<form method="post" action="{{ path('app_feedback_admin_delete', {'id': feedback.id}) }}" onsubmit="return confirm('{{ 'Are you sure you want to delete this item?' | trans }}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ feedback.id) }}">
    <button class="btn">{{ 'Delete' | trans }}</button>
</form>
