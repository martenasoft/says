    <!-- Другие URL страниц -->
    <url>
        <loc>https://www.example.com/page2</loc>
        <lastmod>2022-03-24</lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.7</priority>
    </url>

    <!-- Другие URL страниц -->
    <!-- ... -->

</urlset>
